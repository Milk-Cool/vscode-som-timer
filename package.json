{
  "name": "vscode-som-timer",
  "displayName": "Summer of Making timer",
  "description": "Summer of Making countdown and hackatime stats",
  "version": "0.1.1",
  "publisher": "milkcool",
  "homepage": "https://github.com/Milk-Cool/vscode-som-timer/blob/master/README.md",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Milk-Cool/vscode-som-timer.git"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "som-timer",
          "title": "Summer of Making",
          "icon": "public/logo_bito.svg"
        }
      ]
    },
    "views": {
      "som-timer": [
        {
          "type": "webview",
          "id": "somTimer.openview",
          "name": "Timer",
          "contextualTitle": "Timer",
          "icon": "public/logo_bito.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "somTimer.openview",
        "title": "Open Summer of Making timer"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "typescript": "^4.4.3"
  }
}
